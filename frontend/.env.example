# Example environment variables - Copy to .env and fill in your values

# API Configuration
VITE_API_BASE_URL=https://your-api-gateway-url.amazonaws.com/prod

# AWS Configuration
VITE_AWS_REGION=us-east-1

# Cognito Configuration
VITE_USER_POOL_ID=your-user-pool-id
VITE_USER_POOL_CLIENT_ID=your-user-pool-client-id
VITE_IDENTITY_POOL_ID=your-identity-pool-id

# AWS RUM Configuration (will be populated after infrastructure deployment)
VITE_RUM_APPLICATION_ID=your-rum-application-id
VITE_RUM_IDENTITY_POOL_ID=your-rum-identity-pool-id
VITE_RUM_REGION=us-east-1